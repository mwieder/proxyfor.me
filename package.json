{
  "name": "proxyfor.me",
  "version": "0.1.0",
  "author": "gm@proxyfor.me",
  "license": "ISC",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "b": "cd ui && browserify -s main src/main.js > dist/bundle.js",
    "u": "cd ui && uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "g1": "cd ui/dist && gzip -k -f bundle.min.js",
    "g2": "cd ui/src && gzip -k -f *.css *.html && mv *.gz ..",
    "build": "npm run b && npm run u && npm run g1 && npm run g2"
  },
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/forms": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "@types/body-parser": "0.0.34",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.34",
    "@types/marked": "0.0.28",
    "@types/method-override": "0.0.29",
    "@types/mongodb": "^2.1.36",
    "aws-sdk": "^2.28.0",
    "body-parser": "^1.14.1",
    "core-js": "^2.4.1",
    "cron": "^1.2.1",
    "errorhandler": "^1.4.2",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.2.1",
    "marked": "^0.3.6",
    "method-override": "^2.3.5",
    "mongodb": ">= 1.4.29",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "systemjs": "^0.20.9",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "browserify": "^14.1.0",
    "typescript": "^2.3.3",
    "uglifyjs": "^2.4.10"
  }
}
